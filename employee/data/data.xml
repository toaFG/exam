<odoo>
    <data noupdate="1">
        <!-- Adding data for employee skill types -->
        <record id="employee_skill_type_1" model="employee.skills.type">
            <field name="name">Technical Skills</field>
        </record>

        <record id="employee_skill_type_2" model="employee.skills.type">
            <field name="name">Management Skills</field>
        </record>

        <record id="employee_skill_type_3" model="employee.skills.type">
            <field name="name">Communication Skills</field>
        </record>

        <!-- Adding data for employee skill levels -->
        <record id="employee_skill_level_1" model="employee.skills.level">
            <field name="name">Advanced Level</field>
            <field name="th_skill_type_id" ref="employee_skill_type_1"/>
        </record>

        <record id="employee_skill_level_2" model="employee.skills.level">
            <field name="name">Intermediate Level</field>
            <field name="th_skill_type_id" ref="employee_skill_type_2"/>
        </record>

        <record id="employee_skill_level_3" model="employee.skills.level">
            <field name="name">Basic Level</field>
            <field name="th_skill_type_id" ref="employee_skill_type_3"/>
        </record>

    </data>
</odoo>
